generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model ServiceCategory{
  id Int @id @default(autoincrement())
  name String
  icon String
  color String
}

model ServiceProviderInformation{
  id Int @id @default(autoincrement())
  name String
  description String?
  imageUrl String?
  phone String?
  email String?
  town String
  city String
  state String
  country String
  category String
}


